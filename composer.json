{
  "name": "gamajo/codeception-redirects",
  "description": "Codeception Redirects Module",
  "license": "MIT",
  "homepage": "https://github.com/GaryJones/codeception-redirects",
  "authors": [
    {
      "name": "Gary Jones",
      "homepage": "https://gamajo.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/gamajo/codeception-redirects/issues",
    "source": "https://github.com/gamajo/codeception-redirects"
  },
  "config": {
    "sort-order": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.0",
    "codeception/codeception": "^2.5 || ^3.0",
    "roave/security-advisories": "dev-master"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.4",
    "squizlabs/php_codesniffer": "^3.1",
    "wimg/php-compatibility": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Codeception\\Module\\": "src/"
    }
  },
  "scripts": {
    "install-codestandards": [
      "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
    ]
  }
}
